{
    "name": "Microsoft.PowerShell.LocalAccounts",
    "version": "1.0.0-*",

    "buildOptions": {
        "debugType": "portable",
        "keyFile": "../signing/visualstudiopublic.snk",
        "delaySign": true,
        "publicSign": false,
        "warningsAsErrors": true
    },

    "dependencies": {
        "System.Management.Automation": "1.0.0-*"
    },

    "frameworks": {
        "netstandard1.6": {
            "imports": [ "dnxcore50" ],
            "buildOptions": {
                "define": [ "CORECLR" ]
            },
            "dependencies": {
                "System.Net.NameResolution": "4.0.0"
            }
        },
        "net451": {
        }
    }
}
